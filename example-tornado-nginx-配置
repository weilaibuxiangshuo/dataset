//由于使用tornado是前后端分离，所以可以直接设置root的路径为前端的index.html存放的地方

upstream payout {
    server 127.0.0.1:8009;
}

server {
    listen       80;
    server_name  localhost;

    root /home/fastpay/statics;    //直接在外面设置前端index.html文件位置
    index index.html index.htm;


    location / {
        proxy_pass http://payout;
        #root   /usr/share/nginx/html;
        #index  index.html index.htm;
    }
}
